    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.5.1",
    "mdsvex": "^0.12.6",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
